                <FIELD NAME="task_description" TYPE="text" NOTNULL="true" COMMENT="Task description for the individual student"/>
                <FIELD NAME="assigned_date" TYPE="bigint" LENGTH="20" NOTNULL="true" COMMENT="Timestamp when the task is assigned"/>
                <FIELD NAME="due_date" TYPE="bigint" LENGTH="20" NOTNULL="true" COMMENT="Timestamp for the due date of the task"/>
                <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="incomplete" COMMENT="Task status (incomplete, in progress, completed, overdue)"/>
                <FIELD NAME="completion_date" TYPE="bigint" LENGTH="20" NOTNULL="false" COMMENT="Timestamp when the task is completed"/>
            </FIELDS>
            <KEYS>
