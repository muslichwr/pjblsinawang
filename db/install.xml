        <TABLE NAME="pjbl_sinawang_subtasks" COMMENT="Table to store subtasks for pjbl_sinawang">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" COMMENT="Primary key"/>
                <FIELD NAME="taskid" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Task ID"/>
                <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="true" COMMENT="Title of the subtask"/>
                <FIELD NAME="description" TYPE="text" NOTNULL="true" COMMENT="Description of the subtask"/>
                <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="To Do" COMMENT="Status of the subtask"/>
                <FIELD NAME="duedate" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Due date of the subtask"/>
                <FIELD NAME="is_completed" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" COMMENT="Subtask completion status"/>
                <FIELD NAME="created_at" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Timestamp when the subtask was created"/>
                <FIELD NAME="updated_at" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Timestamp when the subtask was last updated"/>
                <FIELD NAME="cmid" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Course Module ID"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key"/>
                <KEY NAME="taskid_fk" TYPE="foreign" FIELDS="taskid" REFTABLE="pjbl_sinawang_tasks" REFFIELDS="id" ONDELETE="cascade" COMMENT="Foreign key referencing pjbl_sinawang_tasks(id)"/>
            </KEYS>
        </TABLE>
